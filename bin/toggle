#!/bin/bash
touch ~/.gitconfig
if [ $(git config --get user.name) == "sranvir" ]; then
    git config --global user.name "ranvir95"
    git config --global user.email "ranvir@ssdnodes.com"

else 
    git config --global user.name "sranvir"
    git config --global user.email "ranvir@numbersandreality.com"
fi
